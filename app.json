{
  "expo": {
    "name": "HandyHubJaV2",
    "slug": "HandyHubJaV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",    
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash/light.png",
        "resizeMode": "contain",
        "backgroundColor": "#f6f6f6",
        "dark": {
          "image": "./assets/splash/dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#212121"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/splash/light.png",
        "resizeMode": "contain",
        "backgroundColor": "#f6f6f6",
        "dark": {
          "image": "./assets/splash/dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#212121"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PROFUCT_NAME) accesses your photos to let you set a profile image.",
          "cameraPermission": "Allow $(PROFUCT_NAME) to access your camera."
        }
      ]
    ],
    "scheme": "handy-hub-ja",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fd43bdcc-00aa-470c-a17c-4bc1427d605a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fd43bdcc-00aa-470c-a17c-4bc1427d605a"
    }
  }
}
